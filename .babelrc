{
    "presets": [
        ["@babel/preset-env",{"targets": {"node": "current"}}],
        "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "module-resolver", 
        {
          "root": ["./"],
          "alias": {
            "types": "./src/types",
            "utils": "./src/utils"
          }
        }
      ]
    ],
    "env": {
        "production": {
          "plugins": [
            ["transform-remove-console", { "exclude": ["error"] }],
            "transform-react-remove-prop-types"
          ]
        }
      }
  }